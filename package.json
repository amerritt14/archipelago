{
  "name": "Archipelago",
  "version": "0.1.0",
  "main": "app/main/init.js",
  "dependencies": {
    "chokidar": "^1.7.0",
    "coffeescript": "^2.1.0",
    "default-shell": "^1.0.1",
    "is-hotkey": "^0.1.1",
    "material-components-web": "^0.27.0",
    "nested-property": "0.0.7",
    "node-pty": "^0.7.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-split-pane": "^0.1.74",
    "rmwc": "0.0.1-rc12"
  },
  "description": "A terminal built on electron",
  "repository": "https://github.com/npezza93/archipelago",
  "author": "npezza93 <npezza93@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "electron": "^1.8.1",
    "electron-builder": "19.45.0"
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --publish onTagOrDraft"
  },
  "build": {
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "TerminalEmulator"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "mac": {
      "category": "public.app-category.developer-tools"
    }
  }
}
