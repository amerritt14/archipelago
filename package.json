{
  "name": "Archipelago",
  "version": "0.1.0",
  "main": "app/main/init.js",
  "dependencies": {
    "chokidar": "^1.7.0",
    "coffeescript": "^2.0.2",
    "default-shell": "^1.0.1",
    "electron-builder-squirrel-windows": "^19.45.0",
    "material-components-web": "^0.21.1",
    "nested-property": "0.0.7",
    "node-pty": "^0.7.2",
    "xterm": "^2.9.2"
  },
  "description": "A terminal built on electron",
  "repository": "https://github.com/npezza93/archipelago",
  "author": "npezza93 <npezza93@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "electron": "^1.8.1",
    "electron-builder": "^19.45.0"
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder -mwl"
  },
  "build": {
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category":"System"
    },
    "win": {
      "target": "squirrel",
      "icon": "build/icon.ico"
    }
  }
}
